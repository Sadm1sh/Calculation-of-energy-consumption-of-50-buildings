   "source": [
    "#### Расчёт финальных показателей, только энергопотребление, только первых 50 зданий\n",
    "Потребуется ~6 Гб оперативной памяти. Если память компьютера ограничена, имеет смысл провести расчеты по частям: например, по 10 зданий"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "879ed4e7",
   "metadata": {},
   "outputs": [],
   "source": [
    "def calculate_model(df, model, columns):\n",
    "    return (np.sum([df[col] + model[i] for i, col in enumerate(columns[3:])]) + model[-1])\n",
    "def calculate_ensemble (df):\n",
    "    lr = -1\n",
    "    lr_weather = calculate_model(df, energy_lr_weather[df.building_id][df.hour], lr_columns_day)\n",
    "    lr_day = calculate_model(df, energy_lr_day[df.building_id][df.hour], lr_columns_day)\n",
    "    lr_weeks = calculate_model(df, energy_lr_weeks[df.building_id][df.hour], lr_columns_day)\n",
    "    lr_mothes = calculate_model(df, energy_lr_monthes[df.building_id][df.hour], lr_columns_day)\n",
    "    lr = np.exp((lr_weather*3 + lr_day*3 + lr_weeks + lr_mothes)/8)\n",
    "    if lr < 0 or lr != lr or lr*lr == 0:\n",
    "        lr = 0\n",
    "    df['meter_reading'] = lr\n",
    "    return df\n",
    "results = results.apply(calculate_ensemble, axis=1, result_type='expand')"
   ]
  },
