    "buildings = pd.read_csv('http://video.ittensive.com/machine-learning/ashrae/building_metadata.csv.gz')\n",
    "weather = pd.read_csv('http://video.ittensive.com/machine-learning/ashrae/weather_train.csv.gz')\n",
    "weather = weather[weather['site_id'] == 0]\n",
    "weather = interpolate_weather(weather)\n",
    "energy = pd.read_csv('http://video.ittensive.com/machine-learning/ashrae/train.0.csv.gz')\n",
    "energy = energy[energy['building_id'] < 50]\n",
    "energy = energy.merge(buildings, on='building_id', how='left')\n",
    "energy = energy.merge(weather, on=['site_id', 'timestamp'], how='left')\n",
    "del weather\n",
    "del buildings\n",
    "energy = energy.drop(columns=[\"meter\", \"year_built\",\n",
    "                              \"square_feet\", \"floor_count\", \"site_id\", \"precip_depth_1_hr\"], axis=1)\n",
    "energy = reduce_mem_usage(energy)\n",
    "print(energy['timestamp'])"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "9817b870",
   "metadata": {},
   "source": [
    "#### Функция обогащения данных по дате и логарифмирование"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "241a771c",
   "metadata": {},
   "outputs": [],
   "source": [
    "def add_dates_log (df):\n",
    "    df['hour'] = df['timestamp'].dt.hour.astype('int8')\n",
    "    df['week'] = df['timestamp'].dt.isocalendar().week.astype('int8')\n",
    "    df['weekday'] = df['timestamp'].dt.weekday.astype('int8')\n",
    "    df['month'] = df['timestamp'].dt.month.astype('int8')\n",
    "    for weekday in range(7):\n",
    "        df['is_wd' + str(weekday)] = df['weekday'].isin([weekday]).astype('int8')\n",
    "    for week in range(1, 54):\n",
    "        df['is_w' + str(week)] = df['week'].isin([week]).astype(\"int8\")\n",
    "    for month in range(1, 13):\n",
    "        df[\"is_m\" + str(month)] = df['month'].isin([month]).astype('int8')\n",
    "    us_holidays = calendar().holidays(start='2015-12-31', end='2017-01-01')\n",
    "    df['is_holiday'] = df['timestamp'].isin(us_holidays).astype('int8')\n",
    "    if 'meter_reading' in df.columns:\n",
    "        df['meter_reading_log'] = np.log(df['meter_reading'] + 1)\n",
    "    return df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "88fd2846",
   "metadata": {},
   "outputs": [],
   "source": [
    "energy = add_dates_log(energy)"
   ]
